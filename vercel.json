{
  "version": 2,
  "name": "simulai-frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "frontend/pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "regions": ["gru1"],
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}
